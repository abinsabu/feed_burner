
{% extends 'base.html.twig' %}
{% block body %}
    <div class="wrapper">
        {{ include('side_bar.html.twig') }}
        <!-- Page Content -->
        <div id="content">
            <div class="row">
                <div class="col-sm-6">
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">Total Feeder Count</h5>
                            <div class="alert alert-info" role="alert">
                            <p class="card-text"><b class="ct-1">{{ feedData.feederCount }}</b></p>
                            </div>
                            <a href="{{ path('feeder_list') }}" class="btn btn-info" role="button">Go To Feeder List</a>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title">Total Feed Count</h5>
                            <div class="alert alert-info" role="alert">
                                <p class="card-text"><b class="ct-2">{{ feedData.feedCount }} </b></p>
                            </div>
                            <a href="{{ path('feeds_list_all') }}" class="btn btn-info" role="button">Go To Feed List</a>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
{% endblock %}